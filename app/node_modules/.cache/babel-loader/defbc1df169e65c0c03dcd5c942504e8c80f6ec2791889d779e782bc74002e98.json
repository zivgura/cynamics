style';\n\nimport './App.css';\n\nfunction App() {\n  const [books, setBooks] = useState([]);\n  const [booksToShow, setBooksToShow] = useState([]);\n  const [catalog, setCatalog] = useState([]);\n\n    useEffect(() => {\n        getBooks()\n            .then(data => {\n                setBooks(data);\n                setBooksToShow(data);\n            })\n    }, []);\n\n    function onFavoriteClick(id, isSelected) {\n      if (isSelected) {\n        setCatalog(catalog.filter(b => b.id!== id));\n        setBooksToShow([...booksToShow, books.find(b => b.id === id)])\n      } else {\n        setCatalog([...catalog, books.find(b => b.id === id)]);\n        setBooksToShow(booksToShow.filter(b => b.id!== id));\n      }\n    }\n\n  return (\n    <div className=\"App\">\n      <AppHeader>\n        Book Store\n      </AppHeader>\n      <AppContainer>\n        <BooksContainer>\n          All Books:\n          {booksToShow?.map((book) => (<BookPreview key={book.id} book={book} onFavoriteClick={onFavoriteClick}/>))}\n        </BooksContainer>\n        <BooksContainer>\n          My Catalog:\n          {catalog?.map((book) => (<BookPreview key={book.id} book={book} isSelected={true} onFavoriteClick={onFavoriteClick}/>))}\n        </BooksContainer>\n      </AppContainer>\n    </div>\n  );\n}\n\nexport default App;\n"],"mappings":"AAAA,OAASA,SAAS,CAAEC,QAAQ,KAAO,OAAO,CAC1C,OAASC,WAAW,KAAQ,2BAA2B,CACvD,OAASC,QAAQ,KAAQ,SAAS,CAClC,OAASC,SAAS,CAAEC,YAAY,CAAEC,cAAc,KAAQ,aAAa,CAErE,MAAO,WAAW,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEnB,QAAS,CAAAC,GAAGA,CAAA,CAAG,CACb,KAAM,CAACC,KAAK,CAAEC,QAAQ,CAAC,CAAGZ,QAAQ,CAAC,EAAE,CAAC,CACtC,KAAM,CAACa,WAAW,CAAEC,cAAc,CAAC,CAAGd,QAAQ,CAAC,EAAE,CAAC,CAClD,KAAM,CAACe,OAAO,CAAEC,UAAU,CAAC,CAAGhB,QAAQ,CAAC,EAAE,CAAC,CAExCD,SAAS,CAAC,IAAM,CACZG,QAAQ,CAAC,CAAC,CACLe,IAAI,CAACC,IAAI,EAAI,CACVN,QAAQ,CAACM,IAAI,CAAC,CACdJ,cAAc,CAACI,IAAI,CAAC,CACxB,CAAC,CAAC,CACV,CAAC,CAAE,EAAE,CAAC,CAEN,QAAS,CAAAC,eAAeA,CAACC,EAAE,CAAEC,UAAU,CAAE,CACvC,GAAIA,UA
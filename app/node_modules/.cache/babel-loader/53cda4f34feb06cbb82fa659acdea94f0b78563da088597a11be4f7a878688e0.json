["import { useEffect, useState } from'react';\nimport { BookPreview } from './BookPreview/BookPreview';\nimport { getBooks, sortByTitle } from './utils';\nimport { AppHeader, AppContainer, BooksContainer } from './App.style';\n\nimport './App.css';\n\nfunction App() {\n  const [books, setBooks] = useState([]);\n  const [booksToShow, setBooksToShow] = useState([]);\n  const [catalog, setCatalog] = useState([]);\n\n    useEffect(() => {\n        getBooks()\n            .then(data => {\n                setBooks(data);\n                setBooksToShow(data);\n            })\n    }, []);\n    \n    useEffect(() => {\n      setBooksToShow(booksToShow.sort((a, b) => sortByTitle(a,b)));\n    }, [booksToShow])\n\n    useEffect(() => {\n      setCatalog(catalog.sort((a, b) => sortByTitle(a,b)));\n    }, [catalog])\n\n    function onFavoriteClick(id, isSelected) {\n      if (isSelected) {\n        setCatalog(catalog.filter(b => b.id!== id));\n        setBooksToShow([...booksToShow, books.find(b => b.id === id)])\n      } else {\n        setCatalog([...catalog, books.find(b => b.id === id)]);\n        setBooksToShow(booksToShow.filter(b => b.id!== id));\n      }\n    }\n\n  return (\n    <div className=\"App\">\n      <AppHeader>\n        Book Store\n      </AppHeader>\n      <AppContainer>\n        <BooksContainer>\n          All Books:\n          {booksToShow?.map((book) => (<BookPreview key={book.id} book={book} onFavoriteClick={onFavoriteClick}/>))}\n        </BooksContainer>\n        <BooksContainer>\n          My Catalog:\n          {catalog?.map((book) => (<BookPreview key={book.id} book={book} isSelected={true} onFavoriteClick={onFavoriteClick}/>))}\n        </BooksContainer>\n      </AppContainer>\n    </div>\n  );\n}\n\nexport default App;\n"],"mappings":"AAAA,OAASA,SAAS,CAAEC,QAAQ,KAAO,OAAO,CAC1C,OAASC,WAAW,KAAQ,2BAA2B,CACvD,OAASC,QAAQ,CAAEC,WAAW,KAAQ,SAAS,CAC/C,OAASC,SAAS,CAAEC,YAAY,CAAEC,cAAc,KAAQ,aAAa,CAErE,MAAO,WAAW,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA